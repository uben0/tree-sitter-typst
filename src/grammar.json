{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "typst",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "markup"
    },
    "markup": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "text"
            },
            {
              "type": "SYMBOL",
              "name": "space"
            },
            {
              "type": "SYMBOL",
              "name": "strong"
            },
            {
              "type": "SYMBOL",
              "name": "emph"
            },
            {
              "type": "SYMBOL",
              "name": "raw"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "hash"
                },
                {
                  "type": "SYMBOL",
                  "name": "_expr"
                }
              ]
            }
          ]
        }
      }
    },
    "strong": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "star"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "markup"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "star"
          }
        ]
      }
    },
    "emph": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "underscore"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "markup"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "underscore"
          }
        ]
      }
    },
    "raw": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "raw_delim"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "raw_content"
          },
          "named": true,
          "value": "text"
        },
        {
          "type": "SYMBOL",
          "name": "raw_delim"
        }
      ]
    },
    "_expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "SYMBOL",
          "name": "int"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "text"
    },
    {
      "type": "SYMBOL",
      "name": "space"
    },
    {
      "type": "SYMBOL",
      "name": "star"
    },
    {
      "type": "SYMBOL",
      "name": "underscore"
    },
    {
      "type": "SYMBOL",
      "name": "hash"
    },
    {
      "type": "SYMBOL",
      "name": "ident"
    },
    {
      "type": "SYMBOL",
      "name": "int"
    },
    {
      "type": "SYMBOL",
      "name": "raw_delim"
    },
    {
      "type": "SYMBOL",
      "name": "raw_content"
    }
  ],
  "inline": [],
  "supertypes": []
}
